/**
Для усіх посилань зі сторінки додайте атрибут target="\_blank", якщо
href починається на`https://
 */
$(document).ready(function () {
  // ваш код
  $(document).ready(function () {
    $('a').each(function() {
      if ($(this).attr('href') && $(this).attr('href').startsWith('https://')) {
        $(this).attr('target', '_blank');
      }
    });
  });
});
